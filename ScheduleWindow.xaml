<!-- ScheduleWindow.xaml -->
<Window x:Class="Projet_Grand_Slam_Cuozzo_Ruitenbeek.ScheduleWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Schedule Details" Height="300" Width="400">
    <Grid>
        
            <ComboBox x:Name="comboBoxMatches" HorizontalAlignment="Left" VerticalAlignment="Top" Width="300" Height="30" Margin="10,10,0,0"/>
        <TreeView x:Name="treeViewMatches" HorizontalAlignment="Left" VerticalAlignment="Top" Width="300" Height="200" Margin="10,10,0,0">
            <!-- Vous pouvez personnaliser les propriétés du TreeView selon vos besoins -->
        </TreeView>
    </Grid>
</Window>
